    \draw[-stealth, dashed] (0,.2) -- (2., 1.5) {};
    \node[right] at (2.0, 1.5) {$\theta(0) + 2 \pi f_c t$};
    \node[above] at (.75,1) {$\theta(t)$};
    \node[left] at (0,.2) {$\theta(0)$};
    \plot[domain=-5:9,variable=\x,samples=101,smooth] ({\x},{f2(\x)});
