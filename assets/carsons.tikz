\begin{tikzpicture}[x=16mm,
        every edge/.style = {draw, -Straight Barb},
        declare function={f2(\x)=0.6*(1.2*sin(.2*deg(\x))+1*sin(2*deg(\x))+1.2*sin(1*deg(\x)));
